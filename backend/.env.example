# Copy this file to .env and fill in the values before running in production
# MongoDB connection string (MongoDB Atlas recommended)
MONGO_URI=

# Optional: Port for the server (Render/Railway will usually set PORT automatically)
# PORT=5000

# Admin fallback credentials (used only if no admin exists in DB)
# Create real admins via /admin/create and then remove these
ADMIN_ID=
ADMIN_PASS=
